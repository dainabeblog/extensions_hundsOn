{
  "manifest_version": 2,
  "author": "dainabe",
	"homepage_url": "http://example.com",
  "version": "1.0",
  "name": "【導入】content_scripts",
  "description": "何で導入できないん？？",
  "browser_action": {
    "default_title": "BaclkgroundColor"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["content_scripts.css"],
      "js": ["jquery.min.js", "content_scripts.js"]
    }
  ],
  "background": {
		"scripts": ["background.js"]
	}
}
